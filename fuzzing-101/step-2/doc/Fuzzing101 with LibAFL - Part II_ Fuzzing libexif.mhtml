</span></span><span style="display:flex"><span>      <span style="color:#f92672">"Objectives"</span>: [<span style="color:#e6db74">"MaxMapFeedback"</span>, <span style="color:#e6db74">"CrashFeedback"</span>]
</span></span><span style="display:flex"><span>    },
